<html ng-app="WebSiteapp">
@{
    ViewBag.Title = "ASP.NET5";
}
<head>


</head>

<body ng-controller="WebSiteController">

    <table width="99%" style=" border-bottom:3px solid #3273d5;">
        <tr>

            <td width="190">
                <table width="99%">
                    <tr>
                        <td>
                            Submitted By : Sandeep Kanao

                        </td>


                    </tr>
                </table>
            </td>
            <td class="style1" align="center">
                <h3>Sandeep Kanao - ASP.NET Core 1.0, EF, Angular, MVC 6 with WEB API</h3>


            </td>
            
        </tr>
    </table>

    <table style="width: 99%; background-color:#FFFFFF; border solid 2px #6D7B8D; padding 5px;width 99%;table-layout:fixed;" cellpadding="2" cellspacing="2">

        <tr style="height: 30px; background-color:#336699 ; color:#FFFFFF ;border: solid 1px #659EC7;">
            <td width="40" align="center"><b>WebSite ID</b></td>
            <td width="100" align="center"><b>WebSite Name </b></td>
            <td width="120" align="center"><b>Visits</b></td>
            <td width="120" align="center"><b>Visit Date</b></td>
            

        </tr>
        <tbody data-ng-repeat="details in WebSite">
            <tr>

                <td align="center" style="border: solid 1px #659EC7; padding: 5px;table-layout:fixed;">
                    <span style="color:#9F000F">
                        {{details.WebID}}
                    </span>
                </td>             

                <td align="center" style="border: solid 1px #659EC7; padding: 5px;table-layout:fixed;">
                    <span style="color:#9F000F">
                        {{details.WebName}}
                    </span>
                </td>

                <td valign="top" style="border: solid 1px #659EC7; padding: 5px;table-layout:fixed;">
                    <span style="color:#9F000F">
                        {{details.Visits}}
                    </span>
                </td>
                <td valign="top" style="border: solid 1px #659EC7; padding: 5px;table-layout:fixed;">
                    <span style="color:#9F000F">
                        {{details.VisitDate}}
                    </span>
                </td>
                
            </tr>
        </tbody>
    </table>


</body>
</html>


<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-resource.js"></script>
<script src="~/app.js"></script>

